{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "functions": {
    "api/*": {
      "rewrites": {
        "/doc(.*)": "/doc$1"
      }
    }
  },
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/doc/(.*)",
      "dest": "/doc"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
